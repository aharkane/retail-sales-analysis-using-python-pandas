    "# Did the top 10 selling items have a higher than average discount rate?\n",
    "# top_product_sales = \n",
    "top_product_sales = (\n",
    "    top_product_sales\n",
    "    .assign(\n",
    "        product_dicount_rate = transactions.groupby(['product_id'])['percent_discount'].transform('mean')\n",
